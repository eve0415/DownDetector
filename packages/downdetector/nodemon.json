{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/",
        "dist/",
        "lib/"
    ],
    "watch": [
        "src/",
        "../database/src/",
        "../statuspage/src/"
    ],
    "execMap": {
        "ts": "node -r ts-node/register"
    },
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "ts"
}
